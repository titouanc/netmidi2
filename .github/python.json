{
    "problemMatcher": [
        {
            "owner": "ruff",
            "pattern": [
                {
                    "regexp": "^([\\w_\\./]+):(\\d+):(\\d+): ([A-Z]+[0-9]+) ([^\\n]+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "mypy",
            "pattern": [
                {
                    "regexp": "^([\\w_\\./-]+):(\\d+): (error): (.+) \\[([^\\]]+)\\]$",
                    "file": 1,
                    "line": 2,
                    "level": 3,
                    "message": 4,
                    "code": 5
                }
            ]
        }
    ]
}
